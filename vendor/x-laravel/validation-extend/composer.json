{
    "name": "x-laravel/validation-extend",
    "description": "Adds rules that may be required that are not in the Laravel validation class.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mehmet ÖĞMEN",
            "email": "www@mehmetogmen.com.tr",
            "homepage": "https://www.mehmetogmen.com.tr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": ">=5.5"
    },
    "autoload": {
        "psr-4": {
            "XLaravel\\ValidationExtend\\": "./src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "XLaravel\\ValidationExtend\\Providers\\ValidationExtendServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
